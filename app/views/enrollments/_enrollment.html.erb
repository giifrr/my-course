<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Course</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    <% @enrollments.each do |enrollment| %>
      <tr>
        <td><%= enrollment.id %></td>
        <td><%= enrollment.user.email %></td>
        <td><%= enrollment.course.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>

